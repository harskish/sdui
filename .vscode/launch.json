    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/Users/erik/opt/miniconda3/envs/ldm/bin/python", // same as ONNX target below
            "processId": "${command:pickProcess}",
            "MIMode": "lldb",
            "setupCommands": [
                { 
                    "text": "-enable-pretty-printing",
                    "description": "enable pretty printing", 
                    "ignoreFailures": true 
                }
            ]
        },
        {
            "name": "ONNX",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/onnx/run.py",
            "console": "integratedTerminal",
            "python": "/Users/erik/opt/miniconda3/envs/ldm/bin/python",
            "justMyCode": true,
            "args": []
        },
        {
            "name": "Interactive",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/interactive.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": []
        },
        {
            "name": "Sample",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/txt2img.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "--plms",
                "--n_samples", "1",
                "--n_rows", "1",
                "--n_iter", "1",
                "--ddim_steps", "50",
                "--prompt", "a cactus with a cowboy hat, cyberpunk, crayons",
            ]
        }
    ]
}